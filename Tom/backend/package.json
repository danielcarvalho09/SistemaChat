{
  "name": "whatsapp-system-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend do Sistema de Atendimento WhatsApp Multi-Tenant",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "./migrate-production.sh",
    "prisma:studio": "prisma studio",
    "clear:messages": "tsx scripts/clear-all-messages.ts",
    "check:data": "tsx scripts/check-data.ts",
    "clean:connections": "tsx scripts/delete-inactive-connections.ts",
    "reset:core": "tsx scripts/reset-core.ts",
    "reset:all": "tsx scripts/reset-all-data.ts",
    "seed": "tsx src/utils/runSeed.ts",
    "promote-admin": "tsx src/utils/promoteAdmin.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "whatsapp",
    "multi-tenant",
    "customer-service",
    "websocket",
    "typescript"
  ],
  "author": "",
  "license": "PROPRIETARY",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.3.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@hapi/boom": "^10.0.1",
    "@prisma/client": "^5.22.0",
    "@whiskeysockets/baileys": "^7.0.0-rc.6",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.28.2",
    "cobe": "^0.6.5",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "file-type": "^19.6.0",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pino": "^10.1.0",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.2",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^5.22.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
